{% extends "layout.html.j2" %}
{% block body %}


<div id="progress">
    {% if not error %}
    <p>Summary generated! Click <a href='{{output_link}}'>here</a> to view it</p>
    {% else %}
    <p>Sorry! We appear to have run into an error: {{ error }}. Please try again later</p>
    {% endif %}
</div>

{% endblock %}